{
  "name": "@chunkflow/server-app",
  "version": "0.0.0",
  "private": true,
  "description": "Complete server application for ChunkFlow Upload SDK",
  "scripts": {
    "build": "nest build",
    "dev": "nest start --watch",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "db:setup": "bash scripts/setup-db.sh"
  },
  "dependencies": {
    "@chunkflow/protocol": "workspace:*",
    "@chunkflow/upload-server": "workspace:*",
    "@fastify/multipart": "^9.4.0",
    "@nestjs/common": "^11.1.12",
    "@nestjs/core": "^11.1.12",
    "@nestjs/platform-fastify": "^11.1.12",
    "fastify": "^5.7.2",
    "pg": "^8.17.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.1.0",
    "@types/pg": "^8.16.0",
    "@types/supertest": "^6.0.2",
    "globals": "^16.0.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  }
}
