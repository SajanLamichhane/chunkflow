{
  "name": "@chunkflow/shared",
  "version": "0.0.0",
  "description": "Shared utilities and tools for ChunkFlow Upload SDK",
  "type": "module",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown src/index.ts --format esm && tsc --emitDeclarationOnly --declaration --outDir dist",
    "dev": "tsdown src/index.ts --format esm --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "upload",
    "utilities",
    "shared",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "mitt": "^3.0.1",
    "p-limit": "^7.2.0",
    "spark-md5": "^3.0.2"
  },
  "devDependencies": {
    "@types/spark-md5": "^3.0.5",
    "fake-indexeddb": "^6.2.5",
    "fast-check": "^4.5.3",
    "happy-dom": "^20.4.0",
    "tsdown": "^0.20.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
